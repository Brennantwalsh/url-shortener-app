<h1>Your links</h1>

<table>
  <tr>
    <th>Slug</th>
    <th>Target URL</th>
    <th>Created At</th>
  </tr>
    <% @links.each do |link| %>
      <% if current_user.id == link.user_id %>
      <tr>
        <td><%= link.slug %></td>
        <td><%= link.target_url %></td>
        <td><%= link.created_at %></td>
      </tr>
      <% end %>
    <% end %>
<table>

